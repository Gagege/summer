{
  "manifest_version": 3,
  "name": "Summer",
  "description": "Instant Table of Contents for any Webpage",
  "version": "1.0",
  "action": {
    "default_popup": "summer.html",
    "default_icon": "images/icon-128.png"
  },
  "icons": {
    "16": "images/icon-16.png",
    "32": "images/icon-32.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  },
  "content_scripts": [
    {
      "js": ["scripts/content.js"]
    }
  ]
}